<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.basedemo">
    <!-- 获取网络状态 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature
        android:name="android.hardware.camera2"
        android:required="true" />

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".mvvm.BindViewActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".cameras.CameraxActivity">

        </activity>
        <activity android:name=".recy.RecyTestActivity"></activity>
        <activity android:name=".animation.Anim_B_Activity" />
        <activity android:name=".animation.Anim_A_Activity" />
        <activity android:name=".jetpac.MyActivity" />
        <activity android:name=".jetpac.NativaActivity" />
        <activity android:name=".arouter.LoginnActivity" />
        <activity android:name=".net.taskNetActivity" />
        <activity android:name=".net.LoadActivity" />
        <activity android:name=".mviews.mViewActivity" />
        <activity android:name=".arouter.Main2Activity" />

        <service android:name=".net.JobService" />

        <activity android:name=".BaseActivity" />
    </application>

</manifest>